{
  "name": "jvc dark theme",
  "title": "jvc dark theme",
  "version": "0.1",
  "repository": "https://github.com/ChevalierSoft/jvc_dark_theme/",
  "homepage": "https://github.com/ChevalierSoft/jvc_dark_theme/",
  "main": "jvc_dark_theme.user.css",
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-config-silverwind": "^5.0.2",
    "perfectionist": "^2.4.0",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "updates": "^9.2.0",
    "versions": "7.0.3"
  },
  "scripts": {
    "authors": "bash tools/authors",
    "clean": "npm run perfectionist && node tools/fix-perfectionist.js",
    "eslint": "eslint tools/*.js",
    "patch": "versions -p -C patch jvc_dark_theme.user.css",
    "minor": "versions -p -C minor jvc_dark_theme.user.css",
    "major": "versions -p -C major jvc_dark_theme.user.css",
    "perfectionist": "perfectionist jvc_dark_theme.user.css jvc_dark_theme.user.css --indentSize 2 --maxAtRuleLength 250",
    "stylelint": "stylelint jvc_dark_theme.user.css",
  }
}
